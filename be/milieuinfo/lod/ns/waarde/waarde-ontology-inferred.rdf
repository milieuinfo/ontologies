    <!-- https://lod.milieuinfo.be/ns/waarde#hoogte -->

    <owl:ObjectProperty rdf:about="https://lod.milieuinfo.be/ns/waarde#hoogte">
        <rdfs:range rdf:resource="https://lod.milieuinfo.be/ns/waarde#Waarde"/>
        <rdfs:comment xml:lang="nl">De hoogte van een voorwerp is de verticale afstand van een
            referentiepunt (zoals een oppervlak) tot het punt waar een voorwerp zich
            bevindt.</rdfs:comment>
